{
  "name": "programmis/yii2-socket-chat",
  "description": "Yii2 socket chat base on simple socket chat",
  "homepage": "https://github.com/programmis/yii2-socket-chat",
  "license": "MIT",
  "type": "extra",
  "authors": [
    {
      "name": "Daniil Sazhin",
      "email": "daniildr22@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "codeception/specify": "0.*",
    "codeception/verify": "0.*",
    "codeception/codeception": "2.*",
    "squizlabs/php_codesniffer": "^2.3",
    "codeception/c3": "2.*"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7",
    "yiisoft/yii2-bootstrap": "*",
    "programmis/socket-chat": "0.*"
  },
  "scripts": {
    "post-install-cmd": "php init --env=Development --overwrite=n",
    "post-update-cmd": "php develop_init.php"
  },
  "autoload": {
    "psr-4": {
      "yii2chat\\": "yii2chat"
    }
  }
}
